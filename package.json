{
    "name": "nbs-rates-parser-api",
    "version": "1.0.0",
    "description": "Node.js API for fetching exchange rates from NBS",
    "main": "src/app.js",
    "scripts": {
        "start": "node src/app.js",
        "dev": "node --watch src/app.js",
        "start-pm2": "pm2 start pm2.config.js",
        "stop-pm2": "pm2 stop nbs-rates-parser-api",
        "logs-pm2": "pm2 logs nbs-rates-parser-api",
        "reset": "rm -rf node_modules && yarn"
    },
    "dependencies": {
        "axios": "^1.6.3",
        "cheerio": "^1.0.0-rc.12",
        "winston": "^3.11.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:marsicdev/nbs-rates-parser-api.git"
    },
    "keywords": [
        "nbs",
        "exchange rates",
        "api"
    ],
    "author": "Marko Arsic",
    "license": "MIT"
}
